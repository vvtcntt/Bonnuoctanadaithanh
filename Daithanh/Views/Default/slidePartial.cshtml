@model IEnumerable<Daithanh.Models.tblImage>
<div id="slide">
    <div id="slideshow">
        <div class="container">
            <div class="c_slider" style="background:@Html.Raw(ViewBag.chuoislide)"></div>
            <div class="slider">
                @foreach (var item in Model)
                {
                    <figure>
                        <a href="@item.Url" title="@item.Name" rel="@item.Link"> <img src="@item.Images" alt="@item.Name" style="width:100% !important" height="352" title="@item.Name" /></a>
                    </figure>
                }
            </div>
        </div>
    </div>

</div>

